@article{leclercqLowdosePenicillinEarly2017,
  title = {Low-Dose Penicillin in Early Life Induces Long-Term Changes in Murine Gut Microbiota, Brain Cytokines and Behavior},
  author = {Leclercq, Sophie and Mian, Firoz M. and Stanisz, Andrew M. and Bindels, Laure B. and Cambier, Emmanuel and {Ben-Amram}, Hila and Koren, Omry and Forsythe, Paul and Bienenstock, John},
