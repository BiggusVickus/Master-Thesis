  keywords = {Animals,Bacteriolysis,bacteriophage,Bacteriophages,Feces,Gastrointestinal Microbiome,Germ-Free Life,gut metabolome,gut microbiome,Metabolome,Mice,Microbial Interactions,microbiota,phage resistance,phage therapy,virus},
