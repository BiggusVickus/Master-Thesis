}

@article{stokar-avihailDiscoveryPhageDeterminants2023,
  title = {Discovery of Phage Determinants That Confer Sensitivity to Bacterial Immune Systems},
  author = {{Stokar-Avihail}, Avigail and Fedorenko, Taya and H{\"o}r, Jens and Garb, Jeremy and Leavitt, Azita and Millman, Adi and Shulman, Gabriela and Wojtania, Nicole and Melamed, Sarah and Amitai, Gil and Sorek, Rotem},
  year = {2023},
  month = apr,
